{
  "expo": {
    "name": "SnackWatch",
    "slug": "snackwatch-fe",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./static/icon.png",
    "splash": {
      "image": "./static/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#efefef"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}